{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "lib": [
      "es2022",
      "dom" // dom is required since @types/node does not contain fetch types
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
